use  northwind;

#1
select CompanyName, ContactName, ContactTitle, Phone from customers
WHERE country = 'France';


#2
SELECT productname, unitprice
 from products
 join suppliers on suppliers.supplierid = products.supplierid
 where suppliers.supplierid = 1;

